{
    "name": "image-to-word-converter/backend",
    "description": "Backend for converting images to Word documents using OCR",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "phpoffice/phpword": "^0.18.0"
    },
    "require-dev": {
        "ext-gd": "*",
        "ext-zip": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpOffice\\PhpWord\\": "vendor/phpoffice/phpword/src/PhpWord/"
        }
    },
    "authors": [
        {
            "name": "Asmit",
            "email": "7mr.anonymous@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 600
    }
}
